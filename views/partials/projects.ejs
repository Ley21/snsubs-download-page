<div class="container text-center" id="contentBody">
    <div id="accordion">
        <% projects.forEach(function(project){ %>
  <div class="card">
    <div class="card-header" id="headingOne">
      <h5 class="mb-0">
        <button class="btn btn-link" data-toggle="collapse" data-target="#<%= project.name %>" aria-expanded="true" aria-controls="<%= project.name %>">
          <h2><%= project.name %></h2>
        </button>
      </h5>
    </div>

    <div id="<%= project.name %>" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
      <div class="card-body">
        <%= project.name %>
      </div>
    </div>
  </div>
        <% }); %>
    </div>
</div>